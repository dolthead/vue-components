{
  "name": "ionic-vue-components",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "serve": "vite dev --host",
    "build": "vue-tsc --noEmit && vite build",
    "e2e": "cypress run",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "serve:pwa": "vite build --mode pwa && vite preview --host --mode pwa",
    "deploy": "vue-tsc --noEmit && vite build && firebase deploy",
    "assets": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'"
  },
  "dependencies": {
    "@capacitor/app": "4.0.1",
    "@capacitor/core": "4.2.0",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "@ionic/core": "^7.8.6",
    "@ionic/vue": "^7.8.6",
    "@ionic/vue-router": "^7.8.6",
    "core-js": "^3.37.0",
    "ionicons": "^6.1.3",
    "swiper": "^9.4.1",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@capacitor/assets": "2.0.4",
    "@capacitor/cli": "4.2.0",
    "@types/node": "^18.19.32",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^8.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^8.7.1",
    "jsdom": "^20.0.3",
    "jshint": "^2.13.6",
    "sass": "^1.77.0",
    "typescript": "^4.9.5",
    "vite": "^4.5.3",
    "vite-plugin-pwa": "^0.14.7",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.8.27"
  },
  "description": "An Ionic project"
}
