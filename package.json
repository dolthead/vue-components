{
  "name": "Ionic Vue",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "serve": "vite dev",
    "build": "vue-tsc --noEmit && vite build",
    "e2e": "cypress run",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "serve:pwa": "vite build --mode pwa && vite preview --host --mode pwa",
    "deploy": "vue-tsc --noEmit && vite build && firebase deploy",
    "assets": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'"
  },
  "dependencies": {
    "@capacitor/app": "4.0.1",
    "@capacitor/core": "4.2.0",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "@ionic/core": "7.0.0-rc.0",
    "@ionic/vue": "7.0.0-rc.0",
    "@ionic/vue-router": "7.0.0-rc.0",
    "core-js": "^3.29.0",
    "ionicons": "^6.1.3",
    "swiper": "^9.1.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/assets": "2.0.4",
    "@capacitor/cli": "4.2.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^8.7.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^8.7.1",
    "jsdom": "^20.0.3",
    "jshint": "^2.13.6",
    "sass": "^1.58.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-pwa": "^0.14.4",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.2.0"
  },
  "description": "An Ionic project"
}
