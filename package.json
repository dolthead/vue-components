{
  "name": "ionic-vue-components",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "description": "An Ionic project",
  "scripts": {
    "serve": "vite dev --host",
    "build": "vue-tsc --noEmit && vite build",
    "e2e": "cypress run",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "serve:pwa": "vite build --mode pwa && vite preview --host --mode pwa",
    "deploy": "vue-tsc --noEmit && vite build && firebase deploy",
    "assets": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'"
  },
  "dependencies": {
    "@capacitor/app": "6.0.0",
    "@capacitor/core": "6.0.0",
    "@capacitor/haptics": "6.0.0",
    "@capacitor/keyboard": "6.0.0",
    "@capacitor/status-bar": "6.0.0",
    "@ionic/core": "^8.4.1",
    "@ionic/vue": "^8.4.1",
    "@ionic/vue-router": "^8.4.1",
    "core-js": "^3.40.0",
    "ionicons": "^7.4.0",
    "swiper": "^9.4.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@capacitor/assets": "3.0.5",
    "@capacitor/cli": "6.0.0",
    "@cypress/request": "^3.0.7",
    "@types/node": "^18.19.71",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^8.7.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^8.7.1",
    "jsdom": "^20.0.3",
    "jshint": "^2.13.6",
    "sass": "^1.83.4",
    "semver": "^7.6.3",
    "typescript": "^4.9.5",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.8.27",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  }
}
